{
  "plugins": {
    "lint": {
      "presets": [
        "remark-preset-lint-dealerdirect"
      ]
    },
    "validate-links": {
    }
  },
  "settings": {
    "commonmark": true,
    "gfm": true,
    "yaml": true,
    "rule": "-",
    "ruleSpaces": false,
    "ruleRepetition": 70,
    "emphasis": "*",
    "listItemIndent": "1",
    "incrementListMarker": false,
    "spacedTable": false
  }
}